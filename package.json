{
  "name": "@leighton-digital/cloud-blocks",
  "version": "1.2.1",
  "description": "A collection of AWS CDK L3 custom opinionated constructs to build more efficiently and securely.",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "images/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leighton-digital/cloud-blocks.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leighton-digital/cloud-blocksp/issues"
  },
  "engines": {
    "node": ">=22.11",
    "pnpm": ">=10.15.0"
  },
  "publishConfig": {
    "tag": "latest",
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "generate": "plop",
    "lint": "biome check --linter-enabled=true --formatter-enabled=false .",
    "format": "biome format --write .",
    "format:fix": "biome format --write --fix .",
    "format:check": "biome check --formatter-enabled=true --linter-enabled=false .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "spell:check": "cspell .",
    "spell:check-files": "cspell --no-must-find-files --files",
    "commit": "cz",
    "build": "rm -f tsconfig.tsbuildinfo && rm -rf ./dist && tsc -p tsconfig.json",
    "test": "jest",
    "docs:ref": "rm -rf ./docs/reference && typedoc",
    "docs:post": "node scripts/configure-typedoc.mjs",
    "docs:build": "pnpm --prefix docs-site run build",
    "docs:serve": "pnpm --prefix docs-site run serve",
    "dev:setup": "pnpx lefthook install",
    "prepack": "npm run build",
    "prepublishOnly": "npm test"
  },
  "type": "commonjs",
  "packageManager": "pnpm@10.15.0",
  "homepage": "https://github.com/leighton-digital/cloud-blocks#readme",
  "keywords": [
    "CDK",
    "AWS"
  ],
  "author": "leighton digital",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@changesets/cli": "2.29.6",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@cspell/dict-en-gb": "5.0.15",
    "@docusaurus/core": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/theme-classic": "3.8.1",
    "@docusaurus/theme-mermaid": "3.8.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@swc/jest": "0.2.39",
    "@types/jest": "30.0.0",
    "@types/node": "24.7.2",
    "commit-and-tag-version": "12.5.1",
    "commitizen": "4.3.1",
    "cspell": "9.3.0",
    "cz-conventional-changelog": "3.0.1",
    "docusaurus-plugin-typedoc": "1.4.2",
    "esbuild": "0.25.11",
    "jest": "30.2.0",
    "lefthook": "1.13.2",
    "plop": "4.0.2",
    "semantic-release": "24.2.8",
    "ts-node": "10.9.2",
    "typedoc": "0.28.14",
    "typedoc-plugin-frontmatter": "1.3.0",
    "typedoc-plugin-markdown": "4.8.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.182.0",
    "cdk-monitoring-constructs": "9.3.0",
    "cdk-nag": "2.35.62",
    "constructs": "10.0.5"
  },
  "dependencies": {
    "zod": "4.1.12"
  }
}
